{% extends 'emilyBoutikBundle::layout.html.twig' %}

{% block h1 %}
	Me contacter
{% endblock %}

{% block content %}
    <form action="{{ path('emily_contact') }}" method="post" {{ form_enctype(form) }} class="form form-contact">
        {{ form_errors(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.email) }}
        {{ form_row(form.subject) }}
        {{ form_row(form.body) }}
        {{ form_rest(form) }}
        <input type="submit" value="Submit" />
    </form>
{% endblock %}